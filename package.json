{
     "name": "fluh",
  "version": "0.2.0-pre",

  "engines":
  {
    "node": ">= 6"
  },

  "esm": true,

  "scripts":
  {
    "start": "node -r esm start",

    "st":   "eslint index.js lib/ test/ perf/",
    "unit": "mocha -r esm -r console-ultimate -r test/globals test/*.test.js",
    "test": "npm run st && npm run unit",

    "cover": "gulp --once && cd release/dev && nyc -r text -r html npm run unit",

    "perf": "node -r esm perf/perf",

    "build": "gulp --final"
  },

  "files":
  [
    "lib/",
    "map/",
    "thru/",
    "index.js"
  ],

  "dependencies":
  {
    "def-prop":
      "1"
  },

  "devDependencies":
  {
    "esm":
      "^3.2",


    "gulp":
      "4",
 
    "rollup":
      "^1.27",
 
    "metalpipe":
      "1",


    "eslint":
      "6",

    "js-outlander":
      "StrangeTransistor/js-outlander#2.5.1",

    "console-ultimate":
      "3",


    "mocha":
      "6",

    "chai":
      "4",

    "sinon":
      "7",


    "nyc":
      "13",


    "benny":
      "^3.6",


    "flyd":
      "0.2"
  }
}